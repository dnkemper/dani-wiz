{
    "name": "acquia/drupal-recommended-project",
    "description": "Acquia-compatible Drupal application based on the Drupal Recommended Project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Acquia Engineering",
            "homepage": "https://www.acquia.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "acquia/acquia_cms": "^1.3.2",
        "acquia/blt": "^13.2",
        "acquia/blt-drupal-test": "^1.0",
        "acquia/blt-multisite": "^1.0",
        "acquia/blt-travis": "^1.0",
        "acquia/drupal-environment-detector": "^1.4",
        "acquia/http-hmac-php": "3.4.0",
        "acquia/memcache-settings": "^1.1",
        "bower-asset/chosen": "^1.8",
        "bower-asset/fontawesome": "^6.0",
        "bower-asset/lazysizes": "^5.3",
        "bower-asset/photoswipe": "^4.1",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "dompdf/dompdf": "^1.2",
        "drupal/acquia_connector": "^3.0",
        "drupal/acquia_purge": "^1.2",
        "drupal/admin_toolbar": "^3.1",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/allowed_formats": "^1.4",
        "drupal/antibot": "^1.5",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/block_content_permissions": "^1.10",
        "drupal/components": "^2.4",
        "drupal/config_filter": "^2.2",
        "drupal/config_ignore": "^2.3",
        "drupal/config_inspector": "^2.0",
        "drupal/config_split": " ^2.0@beta",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9.3",
        "drupal/default_content": "^2.0@alpha",
        "drupal/devel": "^4.1",
        "drupal/diff": "^1.0",
        "drupal/draggableviews": "^2.0",
        "drupal/editor_advanced_link": "^2.0",
        "drupal/embed": "^1.4",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_embed": "^1.2",
        "drupal/entity_print": "^2.4",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/entity_reference_unpublished": "^1.3",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/externalauth": "^2.0",
        "drupal/field_delimiter": "^1.1",
        "drupal/field_group": "^3.2",
        "drupal/file_mdm": "^2.4",
        "drupal/focal_point": "^1.5",
        "drupal/fontawesome": "^2.19",
        "drupal/fullcalendar": "^1.0@alpha",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^1.4",
        "drupal/heading": "^1.6",
        "drupal/honeypot": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/layout_builder_lock": "^1.1",
        "drupal/layout_builder_operation_link": "^1.0",
        "drupal/layout_builder_restrictions": "^2.12",
        "drupal/layout_builder_shortcuts": "^1.0",
        "drupal/layout_builder_styles": "^1.0@beta",
        "drupal/lazy": "^3.11",
        "drupal/lb_direct_add": "^1.2",
        "drupal/mailsystem": "^4.3",
        "drupal/masquerade": "^2.0@beta",
        "drupal/media_entity_file_replace": "^1.0",
        "drupal/media_library_theme_reset": "^1.1",
        "drupal/media_thumbnails_video": "^1.0@beta",
        "drupal/menu_admin_per_menu": "^1.3",
        "drupal/menu_block": "^1.7",
        "drupal/menu_breadcrumb": "^1.16",
        "drupal/metatag": "^1.19",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/moderated_content_bulk_publish": "^2.0",
        "drupal/mysql56": "^1.3",
        "drupal/node_revision_delete": "^1.0@RC",
        "drupal/node_view_permissions": "^1.5",
        "drupal/oembed_providers": "^2.0",
        "drupal/override_node_options": "^2.6",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.9",
        "drupal/photoswipe": "^3.0",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/redirect": "^1.7",
        "drupal/reroute_email": "^2.1",
        "drupal/robotstxt": "^1.4",
        "drupal/samlauth": "^3.4",
        "drupal/schema_metatag": "^2.2",
        "drupal/search_api_autocomplete": "^1.6",
        "drupal/simple_sitemap": "^4.1",
        "drupal/simplei": "^1.2",
        "drupal/smart_date": "^3.5",
        "drupal/smart_trim": "^1.3",
        "drupal/stage_file_proxy": "^1.2",
        "drupal/superfish": "^1.4",
        "drupal/swiftmailer": "^2.2",
        "drupal/theme_permission": "^1.2",
        "drupal/tvi": "^1.0@RC",
        "drupal/upgrade_status": "^3.13",
        "drupal/views_ical": "^1.0@alpha",
        "drupal/weight": "^3.3",
        "drupal/workbench_email": "^2.2",
        "drush/drush": "^10.6 || ^11",
        "kartsims/easysvg": "^2.4",
        "npm-asset/fontawesome-iconpicker": "^3.2",
        "npm-asset/jquery-collapse": "^1.1",
        "npm-asset/leaflet": "^1.7",
        "oomphinc/composer-installers-extender": "^1.1 || ^2"
    },
    "require-dev": {
        "acquia/coding-standards": "^0.8.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "ergebnis/composer-normalize": true,
            "oomphinc/composer-installers-extender": true
        },
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "allowed-packages": [
                "acquia/acquia_cms"
            ],
            "file-mapping": {
                "[profile-root]/.editorconfig": false,
                "[profile-root]/.gitattributes": false,
                "[profile-root]/.travis.yml": false,
                "[profile-root]/acquia-pipelines.yml": false,
                "[web-root]/sites/default/default.services.yml": {
                    "mode": "replace",
                    "overwrite": false,
                    "path": "docroot/core/assets/scaffold/files/default.services.yml"
                },
                "[web-root]/sites/default/default.settings.php": {
                    "mode": "replace",
                    "overwrite": false,
                    "path": "docroot/core/assets/scaffold/files/default.settings.php"
                }
            },
            "gitignore": true,
            "locations": {
                "profile-root": "docroot/profiles/contrib/acquia_cms",
                "web-root": "docroot/"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
          "drupal/core": {
          "The Media library doesn't modify the 'media' view if installed in a profile": "https://www.drupal.org/files/issues/2019-05-12/2992056.patch",
          "Site extensions don't get rediscovered after drush cr": "https://www.drupal.org/files/issues/2018-07-11/specify_sitepath_cache_rebuild_extension_discovery-2985199-3.patch",
          "save edits to media embeds": "https://www.drupal.org/files/issues/2019-12-19/3102249-fix-media-ckeditor-snapshop.patch",
          "add img dimensions": "https://www.drupal.org/files/issues/2019-11-19/responsive_image-provide_dimensions_for_image_tag-3095126-4.patch",
          "[PP-1] Reverting entity revisions that contain custom blocks erroneously triggers EntityChangedConstraint": "https://www.drupal.org/files/issues/2021-01-27/3053881-42.patch",
          "Hidden buttons in off-canvas dialog are not being hidden": "https://www.drupal.org/files/issues/2020-02-28/3070375-4.patch",
          "Expose Layout Builder data to REST and JSON:API": "https://www.drupal.org/files/issues/2021-11-22/layout_builder-rebase_drupal_9_3_x-2942975-159.patch",
          "Themes have no context of the entity being rendered in preprocessing a layout when using Layout builder": "https://www.drupal.org/files/issues/2021-09-24/3111192-lb-entity-context-29.patch",
          "Add Views EntityReference filter to be available for all entity reference fields": "https://www.drupal.org/files/issues/2021-12-02/2429699-453-9.3.x.patch",
          "Views Block Display skips preBlockBuild() call on ajax rebuild": "https://www.drupal.org/files/issues/2021-10-13/2605218-70.patch",
          "Periods in query strings are replaced by underscores": "https://www.drupal.org/files/issues/2020-06-25/fix-mangled-query-parameter-names-2984272-36.patch",
          "Fix LB contextual links after dragging block to new section": "https://www.drupal.org/files/issues/2021-12-03/3160785-20.patch"
          },
          "drupal/default_content": {
            "Add a Normalizer and Denormalizer to support Layout Builder": "https://www.drupal.org/files/issues/2020-08-24/default_content-add-normalizer-denormalizer-for-layout-builder-3160146-25.patch",
            "Predictable order of content import": "https://www.drupal.org/files/issues/2020-10-08/3175870.patch"
          },
          "drupal/entity_embed": {
              "Logs flooded with warning messages Invalid display settings encountered": "https://www.drupal.org/files/issues/2019-12-11/3077225-10.reduce-invalid-config-logs.patch"
          },
          "drupal/entity_usage": {
              "fatal error after saveing the page not have a reuse entity": "https://www.drupal.org/files/issues/2021-09-03/3098733-fatal-error-13.patch"
          },
          "drupal/entity_print": {
              "Error on layout builder": "https://www.drupal.org/files/issues/2020-02-06/3111643-1.patch"
          },
          "drupal/inline_entity_form": {
            "Custom blocks created in inline forms should not be reusable": "https://www.drupal.org/files/issues/2020-05-04/3002175-26.patch"
          },
          "drupal/layout_builder_styles": {
            "Section dependency on layout plugin configuration form": "https://www.drupal.org/files/issues/2019-08-23/layout_builder_styles-3062261-%2310.patch",
            "Grouping of layout builder styles": "https://www.drupal.org/files/issues/2021-03-16/3075502-18_0.patch"
          },
          "drupal/menu_block": {
            "Hide block title if menu tree contains no links": "https://www.drupal.org/files/issues/2020-02-12/menu_block-hide_block_if_no_links-2757215-13.patch",
            "Suppress display of regular system menu blocks in Layout Builder": "https://www.drupal.org/files/issues/2019-12-11/menu_block-remove_duplicates-3062278-11.patch"
          },
          "drupal/moderated_content_bulk_publish": {
            "Remove or make optional the node edit form confirmation dialog": "https://www.drupal.org/files/issues/2020-12-01/no_confirmation_dialog-3185848-3.patch"
          },
          "drupal/paragraphs": {
            "Langcode error when switching away from IEF for paragraphs in LB": "https://www.drupal.org/files/issues/2020-06-25/paragraphs-2901390-51.patch",
            "Better error messages for missing referenced entities": "https://www.drupal.org/files/issues/2022-02-10/3095945-paragraphs-error-message-20.patch"
          },
          "drupal/photoswipe": {
            "grouping": "https://www.drupal.org/files/issues/2022-02-07/photoswipe-gallery-group-2625985-28.patch",
            "Way to make lazyload work with photoswipe": "https://www.drupal.org/files/issues/2022-02-09/support_lazy_module-3115456-9.patch"
          },
          "drupal/purge": {
            "Port purge commands to Drush 9": "https://www.drupal.org/files/issues/2921309-17-port-purge-drush-9.patch"
          },
          "drupal/redirect": {
            "Match redirect": "https://www.drupal.org/files/issues/2020-04-13/redirect-n2831605-31.patch"
          },
          "drupal/jquery_ui_touch_punch": {
            "JQuery UI TouchPunch for D9": "https://www.drupal.org/files/issues/2021-02-26/jquery_ui_touch_punch.patch"
          },
          "drupal/better_exposed_filters": {
              "Fixed focus of textfield for bef in views": "https://www.drupal.org/files/issues/2020-09-16/3103626-12.patch"
          }
        },
        "violinist": {
            "allow_updates_beyond_constraint": 0,
            "blocklist": [
                "drupal/core-composer-scaffold"
            ],
            "bundled_packages": {
                "drupal/core-recommended": [
                    "drupal/core-composer-scaffold"
                ]
            }
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "acms:install": [
            "bash ./docroot/profiles/contrib/acquia_cms/install-acms"
        ],
        "nuke": [
            "rm -rf docroot vendor composer.lock"
        ]
    }
}
